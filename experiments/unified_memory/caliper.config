CALI_SERVICES_ENABLE=aggregate,cupti,cuptitrace,event,mpi,mpireport
CALI_AGGREGATE_KEY=*,mpi.rank
CALI_EVENT_ENABLE_SNAPSHOT_INFO=false
CALI_CUPTITRACE_SNAPSHOT_DURATION=true
CALI_MPIREPORT_CONFIG="select prop:nested,mpi.rank,cupti.activity.kind as \"GPU Activity\",cupti.kernel.name as Kernel,scale(sum#cupti.host.duration,1e-9) as \"Time (host)\",scale(cupti.activity.duration,1e-9) as \"Time (device)\" group by mpi.rank,prop:nested,cupti.activity.kind,cupti.kernel.name format json-split"